{
    "rules": {
        "indent": [
            2,
            4
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "no-console": [0],
        "no-trailing-spaces": [2],
        "eol-last": [2],
        "arrow-spacing": [2],
        "constructor-super": [2],
        "no-const-assign": [2],
        "no-dupe-class-members": [2]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
